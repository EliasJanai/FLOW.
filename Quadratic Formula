#include<stdio.h>
#include<math.h>
//xp=when equation has been added.xs=when equation has been subtracted//
//d =square root part of the equation//
main()
{
    int a,b,c,xp,xs,d;
    printf("enter interger value");
    scanf  ("%d %d %d",&a,&b,&c);
    d=sqrt(b*b-4*a*c);

    xp=b+d/2*a;
    xs=-b-d/2*a;
    if (d<0)
    {
        printf ("\nJANAI");
    }else
    {
    printf ("\nValue of x=%d \nOR",&xp);
    printf ("\nvalue of x=%d",&xs);
}
return 0;
}
